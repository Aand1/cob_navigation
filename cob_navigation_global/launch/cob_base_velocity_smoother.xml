<?xml version="0.0"?>
<launch>

	<!--- Run cob_base_velocity_smoother -->
	<node name="cob_base_velocity_smoother" pkg="cob_base_velocity_smoother" type="cob_base_velocity_smoother" >
		<remap from="input" to="cmd_vel" />
		<remap from="output" to="base_controller/command" />
		<rosparam file="$(find cob_navigation_config)/$(env ROBOT)/cob_base_velocity_smoother_params.yaml" command="load" />
	</node>

</launch>
